   "source": [
    "from pathlib import Path\n",
    "import zipfile\n",
    "\n",
    "with zipfile.ZipFile('aaaaa.zip', 'r') as f:\n",
    "    for fn in f.namelist():\n",
    "        extracted_path = Path(f.extract(fn))\n",
    "        extracted_path.rename(fn.encode('cp437').decode('utf8'))"
   ]
  }
